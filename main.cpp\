#include <iostream>
#include <string>
using namespace std;

int main()
{
string s;
int char_count=0;
getline(cin,s);
for(int i=0;i<s.length();i++)
{
if(int(s[i])==32 || int(s[i])==44 || int(s[i])==46 )//counting character by the help of ascii value of character
{

}
else
char_count++;
}
cout<<char_count;
}
return 0;
}
